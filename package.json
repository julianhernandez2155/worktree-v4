{
  "name": "worktree-v4",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --cache --cache-location .eslintcache",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit --incremental",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:changed": "vitest --changed",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:migrate": "supabase migration up",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "clean": "rm -rf .next out dist coverage .turbo .eslintcache .prettiercache *.tsbuildinfo",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "setup": "node scripts/setup.js"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@tanstack/react-query": "^5.17.0",
    "zustand": "^4.4.7",
    "tailwindcss": "^3.4.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0",
    "date-fns": "^3.0.0",
    "zod": "^3.22.4",
    "react-hook-form": "^7.48.0",
    "@hookform/resolvers": "^3.3.4",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.303.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "recharts": "^2.10.0",
    "react-hot-toast": "^2.4.1",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "vitest": "^1.1.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@playwright/test": "^1.40.0",
    "@next/bundle-analyzer": "^14.1.0",
    "supabase": "^1.142.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "npm-check-updates": "^16.14.11",
    "tsx": "^4.7.0",
    "@vitest/ui": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}